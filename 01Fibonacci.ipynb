    "def fib(n):\n",
    "    if(n<=1):\n",
    "        return n\n",
    "    return fib(n-1)+fib(n-2)\n",
    "\n",
    "def input(n):\n",
    "    i=0\n",
    "    while(i<n):\n",
    "        print(fib(i),end=\" \")\n",
    "        i+=1\n",
    "\n",
    "input(8)"
   ]
